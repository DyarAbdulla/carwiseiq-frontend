# API Configuration
# Backend API base URL (Python FastAPI service)
NEXT_PUBLIC_API_BASE_URL=http://localhost:8000

# Authentication API base URL (Node.js Express service)
# This should point to the auth backend on port 3001
NEXT_PUBLIC_AUTH_API_URL=http://localhost:8000

# PWA Configuration
# Enable PWA service worker in development (set to "true" to enable)
# In production, PWA is automatically enabled
NEXT_PUBLIC_ENABLE_PWA=false
